{
  "name": "gok-dashboard",
  "version": "1.0.0",
  "description": "GOK:AI Dashboard - Command Center for MIGI-Network Ecosystem",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run start-react\" \"wait-on http://localhost:3000 && electron .\"",
    "start-react": "react-scripts start",
    "build": "react-scripts build",
    "build-electron": "npm run build && electron-builder",
    "server": "node src/server/api-server.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "homepage": "./",
  "dependencies": {
    "electron": "^27.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "axios": "^1.5.0",
    "ws": "^8.14.2",
    "cors": "^2.8.5",
    "node-cron": "^3.0.2",
    "systeminformation": "^5.21.8",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.15",
    "@mui/icons-material": "^5.14.15",
    "recharts": "^2.8.0",
    "framer-motion": "^10.16.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "wait-on": "^7.0.1",
    "electron-builder": "^24.6.4"
  },
  "build": {
    "appId": "com.metagenius.gok-dashboard",
    "productName": "GOK:AI Dashboard",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "src/main.js",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "keywords": [
    "MIGI-Network",
    "GOK:AI",
    "Dashboard",
    "Command Center",
    "Consciousness",
    "AGI"
  ],
  "author": "Patryk Sobieranski - META-GENIUSZ-ECOSYSTEM",
  "license": "MIT"
}